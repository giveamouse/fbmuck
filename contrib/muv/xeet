lvar i
lvar count
: loopy
    1 begin
        dup 3 pick <= 
    while
        dup intostr .tell 
        1 + 
    repeat pop

    1 begin
        dup intostr .tell 
        1 +
    (conditional follows)
        dup 3 pick <= 
    not until pop

    1 begin
        dup intostr .tell 
        1 +
    (conditional follows)
        dup 3 pick > 
    until pop

    1 begin
        dup 3 pick <= 
    while
        dup intostr .tell 
    repeat 
;


