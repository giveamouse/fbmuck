    $(LINK32) @<<
  $(LINK32_OPS)  /out:"$(OUTDIR)\restart.exe" $(RSTART_OBJS)
<<



"$(OUTDIR)\fbmuck.exe" : "$(OUTDIR)" "$(INTDIR)" $(NMUCK_OBJS)
