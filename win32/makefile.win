
!IF "$(OS)" == "Windows_NT"
NULL=
!ELSE 
NULL=nul
!ENDIF 

NMSRCDIR=.\winsrc
RSSRCDIR=.\win32
OUTDIR=.\game
INTDIR=.\compile
INCDIR=.\winsrc

CPP=cl.exe
#CPP_OPS=/nologo /MLd /W3 /Gm /GX /ZI /Od /D "_DEBUG" /D "_WIN32_" /D "WIN32" /D "_CONSOLE" /D "_MBCS" /D FD_SETSIZE=4096 /D "HAVE_CONFIG_H" /YX /I"$(INCDIR)" /Fo"$(INTDIR)\\" /Fd"$(INTDIR)\\" /FD /GZ /c
CPP_OPS=/nologo /ML /W3 /GX /O2 /D "_WIN32_" /D "WIN32" /D "NDEBUG" /D "_CONSOLE" /D "_MBCS" /D "FD_SETSIZE=4096" /D "HAVE_CONFIG_H" /I"$(INCDIR)" /Fo"$(INTDIR)\\" /Fd"$(INTDIR)\\" /c
LINK32=link.exe
LINK32_OPS=kernel32.lib user32.lib gdi32.lib winspool.lib comdlg32.lib advapi32.lib shell32.lib ole32.lib oleaut32.lib uuid.lib odbc32.lib odbccp32.lib kernel32.lib user32.lib gdi32.lib winspool.lib comdlg32.lib advapi32.lib shell32.lib ole32.lib oleaut32.lib uuid.lib odbc32.lib odbccp32.lib ws2_32.lib winmm.lib /nologo /subsystem:console /incremental:no /machine:I386
#LINK32_OPS=kernel32.lib user32.lib gdi32.lib winspool.lib comdlg32.lib advapi32.lib shell32.lib ole32.lib oleaut32.lib uuid.lib odbc32.lib odbccp32.lib ws2_32.lib winmm.lib /nologo /subsystem:console /incremental:no /pdb:"$(OUTDIR)\fbmuck.pdb" /debug /machine:I386 /pdbtype:sept 


ALL : "$(OUTDIR)\restart.exe" "$(OUTDIR)\fbmuck.exe"

CLEAN :
        -@erase "$(INTDIR)\*.obj"
	-@erase "$(INTDIR)\vc60.idb"
        -@erase "$(INTDIR)\*.exe"

"$(OUTDIR)" :
    if not exist "$(OUTDIR)/$(NULL)" mkdir "$(OUTDIR)"

"$(INTDIR)" :
    if not exist "$(INTDIR)/$(NULL)" mkdir "$(INTDIR)"


{$(NMSRCDIR)}.cpp{$(INTDIR)}.obj::
   $(CPP) @<<
   $(CPP_OPS) $<
<<

$(INTDIR)\restart.obj : $(RSSRCDIR)\restart.cpp
   $(CPP) $(CPP_OPS) $(RSSRCDIR)\restart.cpp

	
RSTART_OBJS="$(INTDIR)\restart.obj"
